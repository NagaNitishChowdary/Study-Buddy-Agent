# TO CREATE TABLE IN BIGQUERY
CREATE TABLE `your_project.your_dataset.employees` (
  EmployeeId INT64 NOT NULL,
  Name STRING,
  CurrentRole STRING,
  Team STRING,
  CareerGoal STRING,
  TimeAllocated INT64,
  Skills STRING,
  TimeStamp TIMESTAMP DEFAULT CURRENT_TIMESTAMP()
);


CREATE TABLE `upskilling-agent.learning_agent.course_recommendations` (
  EmployeeId INT64,
  CourseName STRING,
  Level STRING,
  Platform STRING,
  Description STRING,
  Link STRING,
  Skill STRING
);